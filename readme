git clone <link>

git add file
#to add all
git add .

git commit -m "msg"
#add + commit
git commit -am "msg"
#to check status
git status
git push


git pull  


git reset --hard  <commit hash>
git reset --hard origin/master


#to know branches
git branch
 
 #to create branch
 git checkout -b "branch_name"
 
